@model Authorization_Module_Domain.Dtos.UserDto.UserDto

@{
    ViewData["Title"] = "Insert";
    Layout = null;
}


<form onsubmit="return formSubmit(this)" method="post">
    <div class="row">
        <div class="col-md-4">
            @Html.ValidationSummary(true)
            <div class="form-group">
                @Html.LabelFor(model => model.UserName, new { @class = "control-label" })
                @Html.EditorFor(e => e.UserName, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(e => e.UserName)
            </div>
        </div>
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>



